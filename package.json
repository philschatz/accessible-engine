{
  "scripts": {
    "build": "tsc",
    "bundle": "rollup --config ./rollup.config.js",
    "clean": "rm ./src/akurraImages.ts ./browser.bundle.js* src/*.js* src/*/*.js*",
    "lint": "eslint . --fix --ext .ts,.tsx",
    "png": "node ./pngExtractor.js > ./src/akurraImages.ts",
    "test": "npm run-script png && npm run-script build && npm run-script start",
    "test:debug": "npm run-script build && npm run-script start:debug",
    "test:audio": "cat gameplay.input.txt | OUTPUT_MODE=${OUTPUT_MODE:-audio} npm run-script start | tee gameplay.log.txt",
    "start": "node -r source-map-support/register ./src/index.js",
    "start:debug": "node --inspect-brk -r source-map-support/register ./src/index.js"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/ansi-escapes": "^4.0.0",
    "@types/ansi-styles": "^3.2.1",
    "@types/node": "^13.1.6",
    "@types/node-hid": "^0.7.3",
    "@types/rbush": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^12.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "pngjs": "^3.4.0",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "ansi-escapes": "^4.3.0",
    "ansi-styles": "^4.2.1",
    "node-hid": "^1.1.0",
    "rbush": "^3.0.1"
  }
}
